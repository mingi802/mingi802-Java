import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
        
        for(;;)
        {
        	int b = sc.nextInt();
        	int c = sc.nextInt();
        	
        	if(b + c == 0)
        	{	
        		break;
        	}
        	System.out.println(b + c);
        }
        sc.close();
	}
}
