import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int bwin = 0; 
		int cwin = 0;
		for(int i = 1; i <= a; i++)
		{
			int b = sc.nextInt();
			int c = sc.nextInt();
			if(b > c)
			{
				bwin += 1;
			}
			else if(b < c)
			{
				cwin += 1;
			}
		}
		System.out.println(bwin + " " + cwin);
		
		sc.close();
	}
}
